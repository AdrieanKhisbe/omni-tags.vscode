{
    "name": "omni-tags",
    "displayName": "Omni Tags",
    "description": "Omni Â§Tags for VSCode",
    "version": "0.0.1",
    "publisher": "adrieankhisbe",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ],
        "grammars": [
            {
                "injectTo": [
                    "text",
                    "source"
                ],
                "scopeName": "inline.omni-tags",
                "path": "./syntaxes/omni-tags.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
